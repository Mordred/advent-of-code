{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "noErrorTruncation": true,
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "paths": {
      "#aoc/*": [ "./*"]
    }
  }
}
